{
  "name": "tailwind-plus-commit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run start:local",
    "start:local": "npm run db:ensure && vite --mode development",
    "start:remote": "vite",
    "build": "tsc && vite build",
    "build:github": "NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "lint": "echo 'Linting disabled for now'",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf .vite package-lock.json",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:status": "supabase status",
    "db:migrate": "supabase db push",
    "db:diff": "supabase db diff",
    "db:generate": "supabase gen types typescript --local > src/lib/database.types.ts",
    "db:studio": "supabase studio",
    "db:logs": "supabase logs",
    "db:ensure": "supabase status || supabase start"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@supabase/supabase-js": "^2.45.4",
    "@tailwindcss/typography": "^0.5.19",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "motion": "^10.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.28.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.8.3",
    "vite": "^5.1.0"
  }
}
